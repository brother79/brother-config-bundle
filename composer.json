{
    "name": "brother/config-bundle",
    "type": "symfony-bundle",
    "version":"1.0",
	"description": "Database-stored settings made available via a service for your Symfony2 project.",
    "keywords": ["symfony2", "config", "admin"],
    "homepage": "http://doc13.ru",
	"license": "MIT",
    "authors": [
        {
            "name":     "Andrey Dashkovskiy",
			"email":    "brother79@mail.ru",
            "homepage": "http://doc13.ru/"
        }
    ],
    "require": {
        "php": ">=5.3.2",
		"symfony/framework-bundle": "~2.1",
        "sonata-project/admin-bundle": "~2.2",
    },
    "require-dev": {
        "doctrine/orm": "~2.3",
		"doctrine/doctrine-bundle": "~1.0",
		"phpunit/phpunit": "~4.1",
		"symfony/assetic-bundle": "~2.1",
		"symfony/symfony": "~2.1"
    },
	"autoload": {
		"psr-0": {
			"Brother\\ConfigBundle": ""
		}
	},
	"target-dir": "Brother/ConfigBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0"
        }
    }
}